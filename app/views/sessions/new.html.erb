<h1 class="text-center mt-4"> Log In</h1>

<%= form_with(scope: :session, url:login_path, local:true) do |f|%>

	<p>
		<%= f.label :Email%><br/>
		<%= f.email_field :email %>
	</p>
    <p>
		<%= f.label :Password%><br/>
		<%= f.password_field :password %>
	</p>
	<p>
    <%=f.submit "Log In" %>
	</p>
<%end%>